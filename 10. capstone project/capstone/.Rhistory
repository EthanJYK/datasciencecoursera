# Paste results
paster <- function(x, y){
t <- substr(x, nchar(x), nchar(x))
if (t == " "){
r <- paste(x, y, " ", sep = "")
} else{
e <- unlist(gregexpr(" ", x))
r <- paste(substr(x, 1, e[length(e)]), y, sep="")
}
return(r)
}
